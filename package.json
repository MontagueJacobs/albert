{
  "name": "sustainable-shop-webapp",
  "version": "1.0.0",
  "description": "Sustainable Shopping Tracker - Albert Heijn Receipt Analysis",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --port 3000",
    "dev:server": "PORT=3001 node server/index.js",
    "build": "vite build",
    "preview": "vite preview",
    "sync": "cd .. && python3 sync_account.py",
    "supabase:seed": "node server/seedSupabaseCatalog.js",
    "supabase:upload-products": "node server/uploadAhProductsToSupabase.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.44.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
